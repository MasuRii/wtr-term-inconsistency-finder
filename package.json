{
  "name": "wtr-lab-term-inconsistency-finder",
  "version": "5.3.2",
  "description": "Finds term inconsistencies in WTR Lab chapters using Gemini AI. Supports multiple API keys with smart rotation, dynamic model fetching, and background processing.",
  "author": "MasuRii",
  "license": "MIT",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/MasuRii/wtr-term-inconsistency-finder.git"
  },
  "bugs": {
    "url": "https://github.com/MasuRii/wtr-term-inconsistency-finder/issues"
  },
  "homepage": "https://github.com/MasuRii/wtr-term-inconsistency-finder",
  "scripts": {
    "build": "webpack --mode=production",
    "build:performance": "webpack --config webpack.performance.config.js --mode=production",
    "build:greasyfork": "webpack --config webpack.greasyfork.config.js --mode=production",
    "build:all": "npm run build:performance && npm run build:greasyfork",
    "dev": "webpack serve --mode=development",
    "dev:performance": "webpack serve --config webpack.performance.config.js --mode=development",
    "dev:greasyfork": "webpack serve --config webpack.greasyfork.config.js --mode=development"
  },
  "devDependencies": {
    "css-loader": "^7.1.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.102.1",
    "webpack-cli": "^6.0.1",
    "webpack-dev-server": "^5.2.2",
    "webpack-userscript": "^3.2.3"
  }
}
